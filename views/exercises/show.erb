
<div class="text-3xl">
  Exercise:
  <%= @exercise[:name] %>
  <br>
</div>
<hr>
<a class="button" href="/exercises/<%= @exercise[:id] %>/edit">Edit</a>
<%- @exercise.sets.each do | set | %>
  <hr>
  reps: <%= set[:reps] %>
  <br>
  weight: <%= set[:weight] %>
  <br>
  date: <%= set.workout[:date].strftime('%b %d, %Y') %>
<%- end %>
